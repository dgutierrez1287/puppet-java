#!/bin/sh


<% if @jdkpath == '' %>JAVA_HOME=<%= scope.lookupvar('openjdk_path') %>
JRE_HOME=$JAVA_HOME
JDK_HOME=<%= scope.lookupvar('openjdk_path') %>
<% else %>JAVA_HOME=<%= scope.lookupvar('jdkpath') %>
JRE_HOME=$JAVA_HOME
JDK_HOME=<%= scope.lookupvar('jdkpath') %>
<% end %>
PATH=$PATH:$JAVA_HOME/bin
export JAVA_HOME JDK_HOME JRE_HOME PATH 